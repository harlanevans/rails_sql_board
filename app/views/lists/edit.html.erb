<h1><i>Edit this List  </i><i class="edit icon"></i></h1>
<%= form_with(model: [@board, @list], local: true, class: "ui form") do |f| %>
  <div class="field" >
    <%= f.label :name %>
    <%= f.text_field :name, required: true, placeholder: "List Name" %>
  </div>
  <div class="field" >
    <%= f.label :date_created %>
    <%= f.date_field :date_created, required: true %>
  </div>
  <%= f.submit nil, class: "ui teal button" %>
  <% end %>